<%= render 'header' %>
<%= render 'navigation' %>
<%= render 'flash' %>
<%= content_tag(:h1) do %>
<% if content_for? :title %>
<%= yield(:title) %>
<% else %>
<%= title %>
<% end %>
<% end %>
<%= content_tag(:main) do %>
<% if content_for?(:sidebar) %>
<%= content_tag(:aside) do %>
<%= yield :sidebar %>
<% end %>
<% end %>
<%= content_tag(:section) do %>
<%= yield %>
<% end %>
<% end %>
<% cache do %>
<%= render 'footer'%>
<% end %>
